import java.awt.*;

import javax.swing.*;


public class Main {
	 private JFrame mainFrame = new JFrame();
	 private JPanel tabPanel;
	 //private JPanel listPanel = new JPanel();
	 
	 private GUI_TabPanel guiTabPanel = new GUI_TabPanel();
	 
	 
	 public void init() {
	
		this.mainFrame.setLayout(new BorderLayout());
		this.mainFrame.setSize(800, 450);
		this.mainFrame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		
		this.addTabPanel();
		this.mainFrame.add(tabPanel, BorderLayout.WEST);
		//this.mainFrame.add(listPanel); 
	 
		this.mainFrame.setVisible(true);
	 }
	 

	 
	 public void addTabPanel() {
		 this.tabPanel = guiTabPanel.tabPanel;
	 }
	 
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		init();
	}

	 
}
